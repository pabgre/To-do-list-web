<%- include("header") -%>


    <div class="box" id="heading">
      <h1><%= webTitle %></h1>
    </div>
    <div class="box">
        <% for (var i = 0; i < newListItems.length; i++) { %>
          <div class="item">
            <input type="checkbox" name="" value="">
            <p><%= newListItems[i]%></p>
          </div>
        <%}%>

        <form class="item" action="/" method="post">
          <input type="text" name="newItem" placeholder="New Item" autocomplete="false">
          <button type="submit" name="list" value = <%= page %>>+</button>
        </form>
    </div>

<%- include("footer") -%>
